{
    "name": "@ulaval/modul-components",
    "version": "1.0.0-beta.93",
    "description": "Component library",
    "author": "Martin Simard <martin.simard@dti.ulaval.ca>",
    "private": false,
    "scripts": {
        "clean": "rimraf dist",
        "reset": "rmdir /S /Q node_modules",
        "dev": "start-storybook --config-dir ./conf/storybook --port 8082",
        "buildWebpack": "webpack --config conf/webpack.config.js --env.lib --colors",
        "build": "tsc --project tsconfig.lib.json && copyfiles -u 1 \"./src/**/*.json\" dist && copyfiles -u 1 \"./src/**/*.html\" dist && copyfiles -u 1 \"./src/**/*.scss\" dist && copyfiles -u 1 \"./src/**/*.css\" dist && npm run assetsToDist && npm run metagenv2",
        "buildlib": "webpack --config conf/webpack.config.js --colors --env.lib",
        "assetsToDist": "copyfiles -u 1 \"./src/assets/**/*\" dist",
        "start": "webpack --config conf/webpack.config.js --env.openshift && http-server \"./dist\" -p 8080 -a 0.0.0.0",
        "unit": "jest --verbose --silent",
        "unitwatch": "jest --watch --verbose --silent",
        "metagen": "node ./tests/meta/meta-generator \"./src/components\" \"./dist/components\" && node ./tests/meta/meta-generator \"./src/mixins\" \"./dist/mixins\"",
        "metagenv2": "node -r ./tests/meta-v2/ts-loader.js  ./tests/meta-v2/generate-meta.ts ./dist",
        "test": "jest --ci -w 1 --silent --coverage --testResultsProcessor=./tests/jest/ci-test-processor && jest -w 1 --ci --silent --config ./conf/storybook/jest.config.storybook.js --verbose",
        "tslint": "tsc --noEmit && tslint --project tsconfig.json -s node_modules/custom-tslint-formatters/formatters -t grouped ",
        "tslint:ci": "tsc --noEmit && tslint --project tsconfig.json -t junit -o reports/junit/js-lint-results.xml ",
        "prepack": "npm run clean && npm run build && npm run buildlib",
        "component": "node ./tests/meta/component-generator",
        "stats": "webpack --config conf/webpack.config.js --env.lib --env.silent --colors --profile --json > reports/stats.json && webpack-bundle-analyzer reports/stats.json",
        "print_version_win": "echo %npm_package_version%",
        "print_version_nx": "echo $npm_package_version",
        "sandboxes": "rimraf dist/**/* && webpack-dev-server --progress --config conf/webpack.config.js  --colors --port 8082 --open --history-api-fallback",
        "storybook:openshift": "start-storybook --ci --port 8080 --config-dir ./conf/storybook",
        "storybook:unit": "jest --config ./conf/storybook/jest.config.storybook.js --verbose",
        "storybook:unit:update": "jest --config ./conf/storybook/jest.config.storybook.js --verbose --updateSnapshot"
    },
    "jest-junit": {
        "output": "reports/junit/js-test-results.xml"
    },
    "jest-html-reporter": {
        "outputPath": "reports/junit/js-test-results.html",
        "includeFailureMsg": true
    },
    "dependencies": {
        "axios": "^0.17.1",
        "cleave.js": "^1.4.7",
        "css-element-queries": "^0.4.0",
        "enquire.js": "^2.1.6",
        "es6-promise": "^4.1.1",
        "filesize": "^3.6.0",
        "froala-editor": "2.9.3",
        "jquery": "^3.3.1",
        "mobile-drag-drop": "^2.3.0-rc.1",
        "moment": "^2.19.3",
        "popper.js": "^1.12.9",
        "portal-vue": "~1.1.1",
        "qs": "^6.5.1",
        "sprintf-js": "^1.1.1",
        "vue-touch": "^2.0.0-beta.4",
        "zingtouch": "^1.0.6"
    },
    "peerDependencies": {
        "vue": "^2.5.9",
        "vue-router": "^3.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@storybook/addon-a11y": "^5.0.6",
        "@storybook/addon-centered": "^5.0.6",
        "@storybook/addon-knobs": "^5.0.6",
        "@storybook/addon-notes": "^5.0.6",
        "@storybook/addon-storyshots": "^5.0.6",
        "@storybook/addon-storysource": "^5.0.6",
        "@storybook/addon-viewport": "^5.0.6",
        "@storybook/vue": "^5.0.6",
        "@types/cleave.js": "^1.4.0",
        "@types/filesize": "^3.6.0",
        "@types/jest": "^23.3.14",
        "@types/node": "^10.12.20",
        "@types/sprintf-js": "^1.1.0",
        "@types/storybook__addon-centered": "^3.3.2",
        "@types/storybook__addon-knobs": "^4.0.4",
        "@types/storybook__addon-notes": "^4.0.2",
        "@types/storybook__addon-storyshots": "^3.4.8",
        "@types/storybook__vue": "^5.0.0",
        "@types/webpack-env": "^1.13.6",
        "@vue/test-utils": "^1.0.0-beta.29",
        "acorn": "^6.0.0",
        "autoprefixer": "^9.4.7",
        "babel-loader": "^8.0.5",
        "babel-plugin-dynamic-import-node": "^2.2.0",
        "babel-plugin-require-context-hook": "^1.0.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-vue": "^2.0.2",
        "copyfiles": "^1.2.0",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.0",
        "custom-tslint-formatters": "^2.4.0",
        "file-loader": "^3.0.1",
        "fork-ts-checker-webpack-plugin": "^0.5.2",
        "html-webpack-plugin": "^3.2.0",
        "http-server": "^0.11.1",
        "husky": "^1.3.1",
        "is-property": "^1.0.2",
        "jest": "^23.6.0",
        "jest-html-reporter": "^2.4.4",
        "jest-junit": "^3.6.0",
        "jest-serializer-vue": "^2.0.2",
        "node-html-parser": "^1.1.10",
        "node-sass": "^4.11.0",
        "postcss-loader": "^3.0.0",
        "rimraf": "^2.6.2",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "stylelint": "^9.3.0",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-scss": "^3.1.3",
        "stylelint-webpack-plugin": "^0.10.5",
        "svg-inline-loader": "^0.8.0",
        "thread-loader": "^2.1.2",
        "ts-jest": "^23.10.5",
        "ts-loader": "^5.3.3",
        "ts-morph": "^1.0.0",
        "ts-node": "^7.0.1",
        "tslint": "^5.14.0",
        "tslint-config-standard": "^8.0.1",
        "typescript": "^3.2.2",
        "typescript-tslint-plugin": "^0.3.0",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "url-loader": "^1.1.2",
        "vue": "^2.5.23",
        "vue-class-component": "^6.1.1",
        "vue-loader": "^15.7.0",
        "vue-property-decorator": "^6.0.0",
        "vue-router": "^3.0.1",
        "vue-server-renderer": "^2.5.23",
        "vue-template-compiler": "^2.5.23",
        "vue-template-loader": "^1.0.0",
        "webpack": "^4.29.3",
        "webpack-bundle-analyzer": "^3.0.4",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.1.14"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ulaval/modul-components.git"
    },
    "engines": {
        "node": ">= 8.0.0",
        "npm": ">= 6.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "files": [
        "src/**/*.ts",
        "dist"
    ],
    "license": "Apache-2.0",
    "husky": {
        "hooks": {
            "commit-msg": "node conf/hooks/commit-msg.js",
            "prepare-commit-msg": "node conf/hooks/prepare-msg.js"
        }
    }
}
