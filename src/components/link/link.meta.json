{
    "tag": "m-link",
    "name": "m-link-meta:name",
    "attributes": {
        "mode": {
            "type": "string",
            "values": [
                "router-link",
                "button",
                "external-link",
                "link"
            ],
            "default": 0
        },
        "url" : {
            "type": "string",
            "default": "/"
        },
        "unvisited" : {
            "type": "boolean",
            "default": false
        },
        "underline" : {
            "type": "boolean",
            "default": true
        },
        "vanilla": {
            "type": "boolean",
            "default": false
        },
        "hiddenText" : {
            "type": "string"
        },
        "icon" : {
            "type": "boolean",
            "default": false
        },
        "iconName" : {
            "type": "string",
            "default": "right-arrow"
        },
        "iconPosition" : {
            "type": "string",
            "values": [
                "left",
                "right"
            ],
            "default": 0
        }
    },
    "overview": "link.overview"
}
